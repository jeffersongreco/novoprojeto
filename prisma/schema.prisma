generator client {
    provider = "prisma-client-js"
}

// Using Vercel Postgres Storage
datasource db {
    provider          = "postgresql"
    url               = env("DATABASE_PRISMA_URL")
    directUrl         = env("DATABASE_URL_NON_POOLING")
    shadowDatabaseUrl = env("DATABASE_URL_NON_POOLING")
}

model Example {
    id          Int     @id @default(autoincrement())
    description String?
}
